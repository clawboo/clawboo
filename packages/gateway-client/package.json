{
  "name": "@clawboo/gateway-client",
  "version": "0.0.0",
  "description": "WebSocket client for the OpenClaw Gateway with typed RPC and event subscription",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/clawboo/clawboo.git",
    "directory": "packages/gateway-client"
  },
  "homepage": "https://github.com/clawboo/clawboo/tree/main/packages/gateway-client",
  "bugs": "https://github.com/clawboo/clawboo/issues",
  "keywords": [
    "clawboo",
    "gateway",
    "websocket",
    "openclaw",
    "rpc"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
