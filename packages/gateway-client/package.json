  },
  "dependencies": {
    "@clawboo/logger": "workspace:*",
    "@noble/ed25519": "^2.1.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@clawboo/tsconfig": "workspace:*",
    "@types/node": "^22.0.0",
    "@types/ws": "^8.5.0",
    "tsup": "^8.4.0",
    "typescript": "^5.7.0"
